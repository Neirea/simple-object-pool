{
    "name": "simple-object-pool",
    "version": "1.0.2",
    "type": "module",
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "require": {
            "types": "./dist/index.cts",
            "default": "./dist/index.cjs"
        }
    },
    "files": [
        "/dist"
    ],
    "scripts": {
        "build:cjs": "tsc --module commonjs && chmod +x ./scripts/cjs.sh && ./scripts/cjs.sh",
        "build:esm": "tsc",
        "build": "rm -rf dist && npm run build:cjs && npm run build:esm",
        "test": "jest",
        "prepublishOnly": "npm run test && npm run build"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.12",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "keywords": [
        "typescript",
        "object-pool",
        "object",
        "pool"
    ],
    "author": {
        "name": "Eugene Shumilin",
        "email": "neirea@ukr.net",
        "url": "https://neirea.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Neirea/object-pool.git"
    }
